    <button class="btn btn-light" id="checking">Check local</button>

    <script>
        $("a").click(function (){
            console.log($(this).html())
        
            $.ajax({
                  url: `/parse/`+$(this).html(),
                  type: "GET",
                  contentType: "application/json",
                  data:"",
                  success: function (data) {
                  },
            });  
        })    
        
        $("#checking").click(function (){
            $.ajax({
                  url: `/parse/check`,
                  type: "GET",
                  contentType: "application/json",
                  data:"",
                  success: function (data) {
                    console.log(data)
                  },
            });  
        })         
    </script>
    
    
    
